{
    "spellright.language": [
        "en"
    ],
    "files.exclude": {
        "**/.vs": true,
        "**/bin": true,
        "**/obj": true,
        "**/.git": true,
        "**/.DS_Store": true,
        "**/**.egg-info": true,
        "**/__pycache__": true,
        "**/**.dist-info": true,
        // "dist": true,
        // "ade-runner/build": true,
        ".venv/include": true,
        ".venv/pyvenv.cfg": true,
        ".venv/lib/python3.10/site-packages/[^a]*": true,
        ".venv/lib/python3.10/site-packages/a[^z]*": true,
        ".venv/lib/python3.10/site-packages/az[^u]*": true,
        // "ade-runner/__init__.py": true,
        // "ade-runner/HISTORY.rst": true,
        // "ade-runner/README.rst": true,
        // "ade-runner/setup.cfg": true,
        // "ade-runner/setup.py": true,
        // "ade-runner/azext_ade_runner/azext_metadata.json": true,
        "LICENSE": true,
    },
    "python.formatting.autopep8Args": [
        "--max-line-length=200"
    ],
    "isort.args": [
        "-l",
        "120",
        "--lbt",
        "1"
    ],
    "restructuredtext.languageServer.disabled": true,
    "restructuredtext.confPath": "",
    "restructuredtext.linter.disabled": true,
    "markdownlint.config": {
        "MD001": false,
        "MD023": false,
        "MD024": false,
        "MD025": false,
        "MD033": false
    }
}